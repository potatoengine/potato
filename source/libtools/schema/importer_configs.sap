module importer_configs;

import common;

[cxximport("up::ImporterConfig", "potato/tools/importer.h")]
struct ImporterConfig {
}

[cxximport("up::JsonImporterConfig", "importers/json_importer.h")]
struct JsonImporterConfig : ImporterConfig {
    [cxxname("type"), json("type")]
    string _type;
}

[cxximport("up::CopyImporterConfig", "importers/copy_importer.h")]
struct CopyImporterConfig : ImporterConfig {
    [cxxname("type"), json("type")]
    string _type;
}
