module importer_configs;

import common;

[cxximport("up::ImporterConfig", "potato/import/importer.h")]
struct ImporterConfig {
}

[cxximport("up::JsonImporterConfig", "json_importer.h")]
struct JsonImporterConfig : ImporterConfig {
    [cxxname("type"), json("type")]
    string _type;
}

[cxximport("up::CopyImporterConfig", "copy_importer.h")]
struct CopyImporterConfig : ImporterConfig {
    [cxxname("type"), json("type")]
    string _type;
}
